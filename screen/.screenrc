escape ^Bb

utf8 on
defencoding UTF-8
encoding UTF-8 UTF-8

altscreen on
defbce on
term alacritty

startup_message off

hardstatus off
hardstatus alwayslastline
# hardstatus string '%{= kG}[ %{G}%H %{g} ][%= %{= kw}%?%-Lw%?%{r}| %{W}%n*%f%t%?(%u)%?%{r} |%{w}%?%+Lw%?%?%= %{g}][%{B} %Y-%m-%d %{W} %c:%s %{g}]'
hardstatus string '%{= kG}[ wtf ]'

# altscreen on
maptimeout 100

scrollback 16384
defscrollback 16384
